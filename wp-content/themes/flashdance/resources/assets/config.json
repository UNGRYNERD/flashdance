      "./scripts/main.js",
      "./styles/main.scss"
    ],
    "customizer": [
      "./scripts/customizer.js"
    ],
    "jkscript": [
      "./scripts/jkscript.js"
    ],
    "countdown": [
      "./scripts/countdown.js"
    ],
    "isotope": [
      "./scripts/isotope.js"
    ],
    "loadmore": [
      "./scripts/loadmore.js"
    ],
